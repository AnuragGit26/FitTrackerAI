{
    "name": "fittrackai",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:api": "tsx scripts/dev-api-server.ts",
        "dev:all": "concurrently \"npm run dev:api\" \"npm run dev\"",
        "build": "vite build",
        "build:prod": "npm run lint:strict && tsc --noEmit && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 1000",
        "lint:check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 1000",
        "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "lint:watch": "eslint . --ext ts,tsx --watch",
        "scrape:exercises": "tsx scripts/scrape-strengthlog.ts",
        "test:e2e": "cypress run",
        "test:e2e:headed": "cypress run --headed",
        "test:e2e:chrome": "cypress run --browser chrome",
        "test:e2e:firefox": "cypress run --browser firefox",
        "test:e2e:edge": "cypress run --browser edge",
        "test:open": "cypress open",
        "test:ci": "start-server-and-test dev http://localhost:3000 test:e2e",
        "test:percy": "percy exec -- cypress run",
        "test:a11y": "cypress run --spec 'cypress/e2e/accessibility/**/*'",
        "test:smoke": "cypress run --spec 'cypress/e2e/00-smoke/**/*'",
        "test:report": "cypress run --reporter mochawesome",
        "test:unit": "vitest",
        "test:unit:run": "vitest run",
        "test:unit:ui": "vitest --ui",
        "test:unit:coverage": "vitest run --coverage",
        "test:component": "cypress run --component",
        "test:component:open": "cypress open --component",
        "test:all": "npm run test:unit:run && npm run test:component && npm run test:e2e"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@hookform/resolvers": "^5.2.2",
        "@react-three/drei": "^9.122.0",
        "@react-three/fiber": "^8.18.0",
        "@vercel/analytics": "^1.6.1",
        "@vercel/functions": "^3.4.0",
        "@vercel/node": "^2.3.0",
        "@vercel/speed-insights": "^1.3.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^2.30.0",
        "dexie": "^3.2.7",
        "firebase": "^12.8.0",
        "firebase-admin": "^13.6.0",
        "framer-motion": "^10.18.0",
        "gsap": "^3.14.2",
        "heic2any": "^0.0.4",
        "lucide-react": "^0.294.0",
        "motion": "^12.29.2",
        "prisma": "^6.19.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "^7.71.1",
        "react-router-dom": "^6.30.3",
        "react-window": "^1.8.11",
        "recharts": "^2.15.4",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "three": "^0.165.0",
        "zod": "^4.3.6",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@percy/cli": "^1.31.8",
        "@percy/cypress": "^3.1.2",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@testing-library/user-event": "^14.6.1",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^20.19.30",
        "@types/puppeteer": "^5.4.7",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "@types/react-window": "^1.8.8",
        "@types/three": "^0.165.0",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "@vitejs/plugin-react": "^4.7.0",
        "@vitest/ui": "^4.0.18",
        "autoprefixer": "^10.4.23",
        "axe-core": "^4.11.1",
        "concurrently": "^9.2.1",
        "cors": "^2.8.6",
        "cypress": "^13.6.2",
        "cypress-axe": "^1.7.0",
        "cypress-file-upload": "^5.0.8",
        "cypress-localstorage-commands": "^2.2.8",
        "cypress-real-events": "^1.15.0",
        "dotenv": "^17.2.3",
        "eslint": "^8.57.1",
        "eslint-plugin-cypress": "^2.15.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.26",
        "express": "^5.2.1",
        "firebase-tools": "^15.4.0",
        "jsdom": "^27.4.0",
        "mochawesome": "^7.1.4",
        "mochawesome-merge": "^4.4.1",
        "mochawesome-report-generator": "^6.3.2",
        "postcss": "^8.5.6",
        "puppeteer": "^24.36.0",
        "start-server-and-test": "^2.1.3",
        "tailwindcss": "^3.4.19",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-pwa": "^1.2.0",
        "vitest": "^4.0.18",
        "workbox-window": "^7.4.0"
    }
}
